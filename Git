which git to check the git path where installed

git config --global user.name "bathinasirisha"
git config --global user.email "sireesha.bathina28@gmail.com"
echo "# Git--Example" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M master
git remote add origin https://github.com/bathinasirisha/Git--Example.git
git push -u origin master

echo foo >> hello
$ git add hello
$ echo bar >> hello
$ git diff
--- a/hello
+++ b/hello
@@ -1,2 +1,3 @@
Hello World!
foo
+bar
$ git diff --staged
--- a/hello
+++ b/hello
@@ -1 +1,2 @@
Hello World!
+foo
$ git diff HEAD
--- a/hello
+++ b/hello
@@ -1 +1,3 @@
Hello World!
+foo
+bar



git checkout -b develop ==create branch
git checkout develp ===switch to 1 to another branch
